@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>
<ul>
    <li>@Model.MembershipType.Name</li>
    @if (Model.Birthdate.HasValue)
            {
                <li>@Model.Birthdate.Value.ToString("d")</li>
            }
</ul>


